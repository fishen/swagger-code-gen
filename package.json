{
  "name": "swagger-code-generate",
  "version": "0.3.0",
  "description": "A swagger code generator for typescript",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require ts-node/register test/**/*.{ts,tsx}",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run type-check&&npm run fix&&webpack",
    "pack": "npm run build&&npm pack"
  },
  "keywords": [
    "swagger",
    "codegen",
    "inject"
  ],
  "author": "fisher <i.yu@qq.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishen/swagger-code-gen.git"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.152",
    "@types/mocha": "^7.0.2",
    "@types/mustache": "^4.0.1",
    "@types/node-fetch": "^2.5.7",
    "bundle-dts": "^1.1.3",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^6.0.1",
    "mocha": "^7.2.0",
    "mp-inject": "^2.5.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "fs-extra": "^9.0.0",
    "lodash": "^4.17.15",
    "mustache": "^4.0.1",
    "node-fetch": "^2.6.0"
  }
}
